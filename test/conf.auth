skynet_root = "./skynet/"
root = "./"

thread = 8

logger = nil

harbor = 0

preload = "./service/preload.lua" -- run preload.lua before every lua service run
start = "test_auth"
bootstrap = "snlua bootstrap"

luaservice = root .. "test/?.lua;" .. skynet_root .. "service/?.lua"
lualoader = skynet_root .. "lualib/loader.lua"

cpath = skynet_root .. "cservice/?.so"

lua_path = root .. "lualib/?.lua;" .. skynet_root .. "lualib/?.lua"
lua_cpath = skynet_root .. "luaclib/?.so"

-- mysql conf
mysql_host = "127.0.0.1"
mysql_port = 3306
mysql_username = "root"
mysql_password = "root"
mysql_database = "skynetdemo"
